{
  "name": "osury",
  "type": "module",
  "description": "Openapi schema to rescript sury schema with types",
  "version": "0.1.5",
  "bin": {
    "osury": "./bin/osury.js"
  },
  "files": [
    "bin/",
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "res:clean": "rescript clean",
    "res:build": "rescript",
    "res:dev": "rescript watch",
    "codegen": "node scripts/codegen.mjs",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build:compile": "esbuild scripts/codegen.js --bundle --platform=node --outfile=dist/codegen.js --format=cjs --external:fs --external:path",
    "build:bundle": "npm run build:compile",
    "prepublishOnly": "npm run res:build && npm run build:bundle"
  },
  "devDependencies": {
    "@rescript/core": "^1.6.1",
    "@rescript/language-server": "^1.66.0",
    "@types/node": "^18.0.0",
    "esbuild": "^0.27.2",
    "gentype": "^4.5.0",
    "jest": "^30.2.0",
    "rescript": "^12.1.0"
  },
  "dependencies": {
    "sury": "^11.0.0-alpha.4",
    "sury-ppx": "^11.0.0-alpha.2"
  }
}
